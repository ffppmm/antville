var deny = this.isDenied(session.user);
if (deny) {
   res.message = deny;
   res.redirect(session.data.referrer ? path.weblog.members.href("login") : path.weblog.href());
}

this.renderPollList(parseInt(req.data.start,10),req.data.show);

res.data.title = path.weblog.title;

res.data.body = this.renderSkinAsString("main");

path.weblog.renderSkin("page");