      var ref = self.opener.document;
      //if (color.indexOf("#") == 0)
      //   color = color.substr(1, color.length-1);
      ref.getElementById("cp1_"+name).value = color;
      ref.getElementById("cp2_"+name).style.backgroundColor = color;
   }
   cancel();
   return;
}

function cancel() {
   self.close();
   return;
}

