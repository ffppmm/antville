checkIfLoggedIn(this.href(req.action));

var deny = this._parent.isEditMembersDenied(session.user);
if (deny) {
   res.message = deny;
   res.redirect(this.site.href());
}

if (req.data.submit == "cancel" || req.data.cancel)
   res.redirect(path.membermgr.href());
else if (req.data.submit == "save" || req.data.save) {
   var result = this.updateMembership(parseInt(req.data.level,10),session.user);
   res.message = result.message;
   if (!result.error)
      res.redirect(path.membermgr.href());
}

res.data.action = this.href(req.action);

res.data.title = this.site.title;

res.data.body = this.renderSkinAsString("edit");

path.site.renderSkin("page");