var deny = path.weblog.isNotPublic(session.user);
if (deny) {
   res.message = deny;
   res.redirect(path.weblog.members.href("login"));
}

if (!path.weblog.showArchive())
   res.redirect(path.weblog.href());

this.renderStorylist(parseInt(req.data.start,10));

res.data.title = path.weblog.title;

res.data.body = this.renderSkinAsString("main");

path.weblog.renderSkin("page");