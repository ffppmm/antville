autoLogin();

var deny = this.isDenied(user);
if (deny) {
   res.message = deny;
   res.redirect(user.cache.referer ? path.weblog.members.href("login") : path.weblog.href());
}

res.skin = "weblog.page";

this.renderStorylist(parseInt(req.data.start,10),req.data.show);

res.data.title = "Antville - " + path.weblog.title;

res.data.body = this.renderSkinAsString("main");