var deny = this.isViewDenied(session.user);
if (deny) {
   res.message = deny;
   res.redirect(this.weblog.href());
}

res.data.title = this.weblog.title;
if (this.title)
  res.data.title += " - " + encode(this.title);

res.data.body = this.renderSkinAsString("results");

path.weblog.renderSkin("page");