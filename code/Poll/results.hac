autoLogin();

var deny = this.isViewDenied(user);
if (deny) {
   res.message = deny;
   res.redirect(this.weblog.href());
}

res.skin = "weblog.page";

res.data.title = "Antville - " + encode(this.weblog.title);
if (this.title)
  res.data.title += " - " + encode(this.title);

res.data.body = this.renderSkinAsString("results");
